		return [
			{id: 'liqpay_color.svg', name: 'coloured'},
			{id: 'liqpay_black.svg', name: 'black'},
			{id: 'liqpay_white.svg', name: 'white', backgroundColor: '#000000'},
			{id: 'liqpay_white_color.svg', name: 'white coloured'},
